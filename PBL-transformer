"\"\"\n","    order = torch.argsort(time, descending=True)\n","    risk = risk[order]\n","    event = event[order]\n","\n","    log_cumsum = torch.logcumsumexp(risk, dim=0)\n","    loss = -(risk - log_cumsum) * event\n","    return loss.sum() / (event.sum() + 1e-8)\n"],"metadata":{"id":"4KAhvO-lIRWb","executionInfo":{"status":"aborted","timestamp":1767460674178,"user_tz":-330,"elapsed":4,"user":{"displayName":"Rishika Ray","userId":"02079523635456342422"}}},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["rows = []\n","\n","for pid, img_path in img_map.items():\n","    if pid not in clinical_df.index:\n","        continue\n","\n","    rows.append({\n","        \"patient_id\": pid,\n","        \"img_path\": img_path,\n","        \"time\": clinical_df.loc[pid, \"time\"],\n","        \"event\": clinical_df.loc[pid, \"event\"],\n","        \"treat_response\": clinical_df.loc[pid, \"treat_response\"]\n","    })\n","\n","master_df = pd.DataFrame(rows)\n","\n","print(\"Master table shape:\", master_df.shape)\n","print(master_df.head())\n","\n","master_df.to_csv(MASTER_CSV, index=False)\n","print(\"Saved:\", MASTER_CSV)\n"],"metadata":{"id":"QWZxmI6JIkD1","executionInfo":{"status":"aborted","timestamp":1767460674183,"user_tz":-330,"elapsed":248163,"user":{"displayName":"Rishika Ray","userId":"02079523635456342422"}}},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["from sklearn.model_selection import train_test_split\n","\n","train_ids, val_ids = train_test_split(\n","    master_df[\"patient_id\"].unique(),\n","    test_size=0.2,\n","    random_state=42\n",")\n","\n","train_df = master_df[master_df[\"patient_id\"].isin(train_ids)].reset_index(drop=True)\n","val_df   = master_df[master_df[\"patient_id\"].isin(val_ids)].reset_index(drop=True)\n","\n","print(\"Train rows:\", train_df.shape)\n","print(\"Val rows:\", val_df.shape)\n"],"metadata":{"id":"B2pyGmrgH41M","executionInfo":{"status":"aborted","timestamp":1767460674196,"user_tz":-330