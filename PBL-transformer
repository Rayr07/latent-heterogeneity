n","        self.clin_proj = nn.Linear(clin_dim, d_model)\n","        self.img_proj  = nn.Linear(img_dim, d_model)\n","\n","        self.cls_token = nn.Parameter(torch.randn(1, 1, d_model))\n","\n","        encoder_layer = nn.TransformerEncoderLayer(\n","            d_model=d_model,\n","            nhead=4,\n","            dim_feedforward=4*d_model,\n","            dropout=0.1,\n","            batch_first=True\n","        )\n","\n","        self.transformer = nn.TransformerEncoder(\n","            encoder_layer,\n","            num_layers=2\n","        )\n","\n","        self.surv_head  = nn.Linear(d_model, 1)\n","        self.treat_head = nn.Linear(d_model, 1)\n","\n","    def forward(self, clin, img):\n","        B = clin.size(0)\n","\n","        clin_tok = self.clin_proj(clin).unsqueeze(1)\n","        img_tok  = self.img_proj(img).unsqueeze(1)\n","        cls      = self.cls_token.expand(B, 1, -1)\n","\n","        tokens = torch.cat([cls, clin_tok, img_tok], dim=1)\n","        z = self.transformer(tokens)[:, 0]\n","\n","        risk = self.surv_head(z).squeeze(1)\n","        response_logit = self.treat_head(z).squeeze(1)\n","\n","        return risk, response_logit\n"],"metadata":{"id":"5dnEtJe8IKk3","executionInfo":{"status":"aborted","timestamp":1767460674243,"user_tz":-330,"elapsed":248202,"user":{"displayName":"Rishika Ray","userId":"02079523635456342422"}}},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["device = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n","\n","model = FusionTransformer(\n","    clin_dim=clinical_df.shape[1]\n",").to(device)\n","\n","opt = torch.optim.Adam(model.parameters(), lr=3e-4)\n","alpha = 0.5\n","\n","for epoch in range(20):\n","    model.train()\n","    total_loss = 0\n","\n","    for b in train_loader:\n","        img = b[\"img\"].to(device)\n","        clin = b[\"clin\"].to(device)\n","        time = b[\"time\"].to(device)\n","        event = b[\"event\"].to(device)\n","        resp = b[\"response\"].to(device)\n","\n","        risk, resp_logit = model(clin, img)\n","\n","        loss_surv = cox_ph_loss(risk, time, event)\n","        loss_resp = F.binary_cross_entropy_with_logits(\n","            resp_logit, resp.float()\n","        )\n","\n","        loss = loss_surv + alpha * loss_resp\n","\n","        opt.zero_grad()\n","        loss.backward()\n","        opt.step()\n","\n","        total_loss += loss.item()\n","\n","    print(f\"Epoch {epoch+1}: loss = {total_loss/len(train_loader):.4f}\")\n"],"metadata":{"id":"f_uMobIPIRNe","executionInfo":{"status":"aborted","timestamp":1767460674247,"user_tz":-330,"elapsed":248205,"user":{"displayName":"Rishika Ray","userId":"02079523635456342422"}}},"execution_count":null,"outputs":[]}]}